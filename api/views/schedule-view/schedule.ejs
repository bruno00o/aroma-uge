        }
        
        .cd-schedule-modal__close::before,
        .cd-schedule-modal__close::after {
            content: '';
            position: absolute;
            top: 50%;
            left: 50%;
            width: 2px;
            height: 22px;
            background: hsl(0, 0%, 100%);
