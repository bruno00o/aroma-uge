        @media (min-width: 64rem) {
            .cd-schedule-modal__header {
                position: absolute;
                display: block;
                top: 0;
                left: 0;
                height: 100%
            }
        }
        
        .cd-schedule-modal__content {
            position: relative;
            z-index: 3;
            display: -ms-flexbox;
            display: flex;
            -ms-flex-align: center;
            align-items: center;
            padding: 0.75em 1.25em;
            padding: var(--space-sm) var(--component-padding)
        }
        
        @media (min-width: 64rem) {
            .cd-schedule-modal__content {
                display: block;
                padding: 0.75em;
                padding: var(--space-sm)
