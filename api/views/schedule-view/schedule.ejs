<!doctype html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script>document.getElementsByTagName("html")[0].className += " js";</script>
    <title><%= prenom + ' ' + nom %> - <%= start + ' - ' + end %></title>
    <meta name="description" content="Emploi du temps de <%= prenom + ' ' + nom %> pour la semaine du <%= start + ' au ' + end %>">
    <meta name="robots" content="noindex, nofollow">
    <link rel="manifest" id="my-manifest-placeholder">
    <meta name="theme-color" content="#090A35" media="(prefers-color-scheme: dark)">
    <meta name="theme-color" content="#FFFFFF" media="(prefers-color-scheme: light)">
    <link rel="apple-touch-icon" href="apple-icon-180.png">
    <script>
        var myDynamicManifest = {
        "name": "Emploi du temps de <%= prenom + ' ' + nom %>",
        "short_name": "EDT <%= prenom + ' ' + nom %>",
        "description": "Emploi du temps de <%= prenom + ' ' + nom %> pour la semaine du <%= start + ' au ' + end %>",
        "start_url": "http://localhost:8080/partage-edt/<%= prenom.toLowerCase() + '.' + nom.toLowerCase() %>/",
        "display": "fullscreen",
        "orientation": "portrait",
        "icons": [{
            "src": "http://localhost:8080/partage-edt/icons/manifest-icon-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "maskable any"
        },
        {
            "src": "http://localhost:8080/partage-edt/icons/manifest-icon-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable any"
        }]
        }
        const stringManifest = JSON.stringify(myDynamicManifest);
        const blob = new Blob([stringManifest], {type: 'application/json'});
        const manifestURL = URL.createObjectURL(blob);
        document.querySelector('#my-manifest-placeholder').setAttribute('href', manifestURL);
    </script>
</head>
    <style>
        /* @import url("https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,600"); */
